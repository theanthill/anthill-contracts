{
  "name": "anthill-contracts",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run compile && npm run migrate",
    "compile": "npx truffle compile",
    "migrate:dev": "npx truffle migrate --network dev --reset | tee deployments/deploy.dev.log",
    "migrate:local-testnet": "npx truffle migrate --network local-testnet --reset | tee deployments/deploy.local-testnet.log",
    "migrate:local-mainnet": "npx truffle migrate --network local-mainnet --reset | tee deployments/deploy.local-mainnet.log",
    "migrate:testnet": "npx truffle migrate --network testnet --reset | tee deployments/deploy.testnet.log",
    "migrate": "npx truffle migrate --network mainnet --reset | tee deployments/deploy.mainnet.log",
    "ganache:dev": "node run-ganache --network dev",
    "ganache:local-testnet": "node run-ganache --network local-testnet --blocktime 3",
    "ganache:local-mainnet": "node run-ganache --network local-mainnet --blocktime 3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theanthill/anthill-contracts"
  },
  "author": "rhlsthrm",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/theanthill/anthill-contracts/issues"
  },
  "homepage": "https://github.com/theanthill/anthill-contracts#readme",
  "devDependencies": {
    "@truffle/artifactor": "^4.0.101",
    "@truffle/contract": "^4.3.20",
    "@truffle/hdwallet-provider": "^1.2.2",
    "@typechain/ethers-v5": "^6.0.2",
    "@typechain/hardhat": "^1.0.0",
    "@types/chai": "^4.2.15",
    "@types/chai-as-promised": "^7.1.3",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.32",
    "bignumber.js": "^9.0.0",
    "dotenv": "^8.2.0",
    "ethereum-waffle": "^3.3.0",
    "ethers": "^5.3.1",
    "prettier": "^2.2.1",
    "prettier-plugin-solidity": "1.0.0-beta.10",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.16",
    "truffle": "^5.3.2",
    "truffle-contract-size": "^2.0.1",
    "ts-generator": "^0.1.1",
    "ts-node": "^9.1.1",
    "typechain": "^4.0.3",
    "typescript": "^4.2.3",
    "yargs": "^4.8.1"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.0.0",
    "@pancakeswap2/pancake-swap-core": "^0.1.0",
    "@theanthill/pancake-swap-periphery": "^1.1.0-beta.1",
    "solc": "^0.8.3"
  }
}
